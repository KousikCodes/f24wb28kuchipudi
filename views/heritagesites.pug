extends layout

block content
  h1= title
  - let results = [{ site_name: "Great Wall of China", location: "China", year_established: 1987 }, { site_name: "Machu Picchu", location: "Peru", year_established: 1983 }, { site_name: "Pyramids of Giza", location: "Egypt", year_established: 1979 }];
  .container-fluid
    .row
      each site in results
        div(class="col-sm-2")
          h3 #{site.site_name}
          p Location: #{site.location}
          p Established: #{site.year_established}
